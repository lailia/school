package chap06.sec4.exam2;

public class KoreanExam1 {

	public static void main(String[] args) {	//실행 클래스
		//Korean 객체 생성
		Korean ko = new Korean("asd", "qqqwe");
		
		//Korean 객체 데이터 읽기
		System.out.println("nation : " + ko.nation);
		System.out.println("name : " + ko.name);
		System.out.println("ssn : " + ko.ssn);

	}

}

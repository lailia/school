package chap05.sec8;

public class MultiArrayExam1 {

	public static void main(String[] args) {
		//2차원 배열 생성
		int[][] scores = {
				{80, 90, 96},
				{76,88}};
		
		System.out.println("length1 : " + scores.length);
		System.out.println("length2 : " + scores[0].length);
		System.out.println("length3 : " + scores[1].length);

		System.out.println("scores[0][0] : " + scores[0][0]);
		System.out.println("scores[0][1] : " + scores[0][1]);
		System.out.println("scores[0][2] : " + scores[0][2]);
		System.out.println("scores[1][0] : " + scores[1][0]);
		System.out.println("scores[1][1] : " + scores[1][1]);

	}

}

package chap13.sec5.exam3;

public static class Util<K,V> {
	public K k;
	public V v;
	
	public static void Util(K k, V v) {
		this.k = k;
		this.v = v;
	}
	
	public void getValue(Util<?,? extends Pair> util, K k) {
		if(k.equals(util.k)) {
			System.out.println(util.v);
		}
		System.out.println(util.v);
		
	}

}

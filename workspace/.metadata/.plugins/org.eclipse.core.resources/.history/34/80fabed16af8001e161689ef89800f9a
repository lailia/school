package chap04.sec8;

public class ForExam3 {

	public static void main(String[] args) {
		
		for(int i = 1; i <= 5; i++) {
			for(int j = 0; j < i; j++) {
				System.out.print("* ");
			}
			System.out.println();
		}

		int x = 0;
		int y = 0;
		
		for(int i = 0; i <= 5; i++) {
			for(int j = 0; j <= 5; j++) {
				System.out.print("* ");
			}
			System.out.println();
		}
		
	}

}

package chap09.sec06.exam3;

import chap09.sec06.exam2.Button.ClickListener;

public class Button {
	//중첩 인터페이스 선언
	//정적 멤버 인터페이스
	public static interface ClickListener{
		//추상 메소드
		void onClick();
		
	}
	
	private ClickListener click;
	
	public void setClick(ClickListener click) {
		this.click = click;
	}
	
	
}

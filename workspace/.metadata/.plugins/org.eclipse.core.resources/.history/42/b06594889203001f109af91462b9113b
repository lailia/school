package chap13.sec3.exam1;

public class GenericExam1 {
	//제네릭 메소드
	public static <T> Box<T> boxing(T t){	//타입 파라미터
		Box<T> box = new Box<T>();
		box.set(t);
		return box;
	}
	
	public static void main(String[] args) {
		//제네릭 메소드인 호출
		Box<Integer> box1 = boxing(100);
		int a1 = box1.get();
		
	}

}

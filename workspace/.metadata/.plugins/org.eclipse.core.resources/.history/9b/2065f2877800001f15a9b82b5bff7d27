package chap09.sec07.exam1;

public class Car {
	//필드에 Tire객체 대입
	private Tire tire1 = new Tire();
	//필드에 익명 자식 객체를 대입
	private Tire tire2 = new Tire() {	//부모 메소드를 재정의

		@Override
		public void roll() {
			System.out.println("익명 자식 Tire 객체1이 구러갑니다");
		}
	};
	
	//필드를 이용한 메소드
	public void run1() {
		tire1.roll();
		tire2.roll();
	}

}

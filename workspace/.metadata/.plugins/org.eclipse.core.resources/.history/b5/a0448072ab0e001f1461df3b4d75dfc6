package chap17.sec04.exam1;

import java.util.*;
import java.util.stream.Stream;

public class StreamExam1 {

	public static void main(String[] args) {
		//List<Product> 컬렉션 생성
		List<Product> list = new ArrayList<>();
		for(int i=1; i<=5; i++) {
			Product product = new Product(i, "상품" + i, "멋진 회사",(int)(10000*Math.random()+1));
			list.add(product);
		}
		
		Stream<Product> stream = list.stream();
		stream.forEach(p -> System.out.println(toString));

	}

}

package chap05.sec6;

public class SplitExam1 {

	public static void main(String[] args) {
		String board = "1,자바 학습,참조타입 String을 학습합니다.,홍길동";
		//문자열 분리
		String[] tokens = board.split(",");
		
		//인덱스별로 읽기
		for(int i = 0; i < tokens.length; i++) {
			System.out.println("tonkes[" + i + "] : " + tokens[i]);
		}

	}

}

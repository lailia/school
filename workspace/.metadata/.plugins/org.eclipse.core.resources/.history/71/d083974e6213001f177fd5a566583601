package chap18.sec04.exam1;

import java.io.*;

public class WriteExam1 {

	public static void main(String[] args) {
		try {
			Writer writer = new FileWriter("C:/Temp/test1.txt");
			//문자 기반의 출력 스트림 생성
			
			char a = 'A';
			writer.write(a);
			char b = 'B';
			writer.write(b);	//1문자씩 출력
			//char 배열 출력
			char[] arr = {'C', 'D', 'E', 'F'};
		}catch(Exception e) {
			e.printStackTrace();
		}

	}

}
